<Window x:Class="CD.DLS.Clients.Controls.Windows.NameChooserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CD.DLS.Clients.Controls.Windows"
        mc:Ignorable="d"
        xmlns:dialogs="clr-namespace:CD.DLS.Clients.Controls.Dialogs"
        Title="Name Chooser" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <dialogs:NameChooser x:Name="NameChooser" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
