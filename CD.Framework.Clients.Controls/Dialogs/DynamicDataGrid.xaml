<UserControl x:Class="CD.DLS.Clients.Controls.Dialogs.DynamicDataGrid"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CD.DLS.Clients.Controls.Dialogs"
             xmlns:self="clr-namespace:CD.DLS.Clients.Controls.Dialogs"
             mc:Ignorable="d" 
             >
    <UserControl.Resources>
        <ControlTemplate x:Key="ToolbarTemplate1" TargetType="{x:Type ToolBar}"/>
    </UserControl.Resources>

    <DataGrid x:Name="grid" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"
              CanUserAddRows="False" AutoGenerateColumns="False" RowEditEnding="grid_RowEditEnding"></DataGrid>
    <!--DataContext="Dt" ItemsSource="{Binding Dt, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"-->
</UserControl>
